{
	"$schema": "https://zed.dev/schema/themes/v0.1.0.json",
	"name": "$name",
	"author": "Kainoa Kanter <kainoa@t1c.dev>",
	"themes": [
		{
			"appearance": "$type",
			"name": "$name",
			"style": {
				"editor.foreground": "$text",
				"editor.background": "$base",
				"editor.gutter.background": "$base",
				"editor.active_line.background": "$surface",
				"editor.line_number": "$muted",
				"editor.active_line_number": "$text",
				"editor.invisible": "$muted",
				"editor.wrap_guide": "$muted44",
				"editor.active_wrap_guide": "$muted88",
				"ghost_element.active": "$rose",
				"ghost_element.hover": "$highlightMed",
				"ghost_element.selected": "$highlightHigh",
				"ghost_element.background": "$highlightLow",
				"border": "$surface",
				"border.variant": "$surface",
				"border.focused": "$surface",
				"border.selected": "$surface",
				"border.transparent": "$surface",
				"border.disabled": "$surface",
				"text": "$text",
				"text.muted": "$muted",
				"text.placeholder": "$muted",
				"text.disabled": "$muted",
				"text.accent": "$foam",
				"surface.background": "$surface",
				"elevated_surface.background": "$surface",
				"panel.background": "$base",
				"panel.focused_border": "transparent",
				"background": "$base",
				"status_bar.background": "$base",
				"title_bar.background": "$base",
				"toolbar.background": "$base",
				"tab_bar.background": "$surface",
				"tab.inactive_background": "$surface",
				"tab.active_background": "$base",
				"element.background": "$surface",
				"element.hover": "$overlay",
				"element.active": "$overlay",
				"element.selected": "$highlightHigh",
				"element.disabled": "$overlay",
				"predictive": "$subtle",
				"modified": "$rose",
				"ignored": "$muted",
				"deleted": "$love",
				"created": "$foam",
				"warning": "$gold",
				"warning.background": "$highlightLow",
				"hint": "$subtle",
				"hint.background": "$highlightLow",
				"error": "$love",
				"error.background": "$highlightLow",
				"error.border": "$love",
				"info": "$pine",
				"scrollbar.thumb.background": "$text44",
				"scrollbar.thumb.border": "transparent",
				"scrollbar.track.background": "transparent",
				"terminal.background": "$base",
				"terminal.ansi.black": "$muted",
				"terminal.ansi.red": "$love",
				"terminal.ansi.green": "$pine",
				"terminal.ansi.yellow": "$gold",
				"terminal.ansi.blue": "$foam",
				"terminal.ansi.magenta": "$iris",
				"terminal.ansi.cyan": "$rose",
				"terminal.ansi.white": "$text",
				"players": [
					{
						"cursor": "$text",
						"background": "$text",
						"selection": "$text22"
					},
					{
						"cursor": "$foam",
						"background": "$foam",
						"selection": "$foam44"
					},
					{
						"cursor": "$iris",
						"background": "$iris",
						"selection": "$iris44"
					},
					{
						"cursor": "$pine",
						"background": "$pine",
						"selection": "$pine44"
					},
					{
						"cursor": "$love",
						"background": "$love",
						"selection": "$love44"
					}
				],
				"syntax": {
					"attribute": {
						"color": "$subtle"
					},
					"boolean": {
						"color": "$rose"
					},
					"comment": {
						"color": "$muted",
						"font_style": "italic"
					},
					"comment.doc": {
						"color": "$subtle"
					},
					"constant": {
						"color": "$text"
					},
					"constructor": {
						"color": "$love"
					},
					"embedded": {
						"color": "$text"
					},
					"emphasis": {
						"color": "$iris",
						"font_style": "italic"
					},
					"emphasis.strong": {
						"color": "$foam",
						"font_weight": 700
					},
					"enum": {
						"color": "$pine"
					},
					"function": {
						"color": "$rose"
					},
					"hint": {
						"color": "$iris"
					},
					"keyword": {
						"color": "$pine"
					},
					"label": {
						"color": "$rose"
					},
					"link_text": {
						"color": "$iris"
					},
					"link_uri": {
						"color": "$pine"
					},
					"number": {
						"color": "$foam"
					},
					"operator": {
						"color": "$subtle"
					},
					"predictive": {
						"color": "$subtle"
					},
					"preproc": {
						"color": "$gold"
					},
					"primary": {
						"color": "$iris"
					},
					"property": {
						"color": "$love"
					},
					"punctuation": {
						"color": "$subtle"
					},
					"punctuation.bracket": {
						"color": "$muted"
					},
					"punctuation.delimiter": {
						"color": "$muted"
					},
					"punctuation.list_marker": {
						"color": "$subtle",
						"font_weight": 700
					},
					"punctuation.special": {
						"color": "$foam"
					},
					"string": {
						"color": "$gold"
					},
					"string.escape": {
						"color": "$gold"
					},
					"string.regex": {
						"color": "$gold"
					},
					"string.special": {
						"color": "$pine"
					},
					"string.special.symbol": {
						"color": "$pine"
					},
					"tag": {
						"color": "$iris"
					},
					"text.literal": {
						"color": "$gold"
					},
					"title": {
						"color": "$love",
						"font_weight": 700
					},
					"type": {
						"color": "$foam"
					},
					"variable": {
						"color": "$text"
					},
					"variable.special": {
						"color": "$foam"
					},
					"variant": {
						"color": "$rose"
					}
				}
			}
		}
	]
}
